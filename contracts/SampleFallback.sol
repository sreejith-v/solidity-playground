pragma solidity 0.8.26;

contract SampleFallback {

    string public text;

    receive() external payable { 
        text = "receive";
    }

    fallback() external payable { 
        text = "fallback";
        payable(msg.sender).transfer(address(this).balance);
    }

    function setText(string memory _text) public {
        text = _text;
    }
}

